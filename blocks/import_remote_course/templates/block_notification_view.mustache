<section id="block_import_remote_notification">
    <h4>{{tamplatename}}</h4>
    <button id="newitems">{{newitems}}</button>
    <ul id="newactivitieslist" class="activitylist hidden">
    {{#newmods}}
        <li draggable="true" data-cmid="{{cmid}}" class="activityitem">{{name}}</li>
    {{/newmods}}
    </ul>
    <button id="updateitems">{{updatesstr}}</button>
    <ul id="updatedactivites" class="activitylist hidden">
    {{#updatemods}}
        <li draggable="true" data-cmid="{{cmid}}">{{name}}</li>
    {{/updatemods}}
    </ul>
    <a id="importsite" target="_blank" href="{{tetstenvurl}}" class="btn">{{# str }} trytemplates, block_import_remote_course {{/ str }}</a>
</section>