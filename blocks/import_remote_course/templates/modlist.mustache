<section id="modlist" style="display:none;">
<span><button type="button" class="btn btn-danger close">X</button><h2>{{# str }} newactheader, block_import_remote_course {{/ str }}</h2></span>
<hr>
<div id="modlisttable">
    <table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>{{# str }} type, block_import_remote_course {{/ str }}</th>
            <th>{{# str }} name, block_import_remote_course {{/ str }}</th>
            <th>{{# str }} privewurl, block_import_remote_course {{/ str }}</th>
            <th>{{# str }} timecreate, block_import_remote_course {{/ str }}</th>
        </tr>
    </thead>
    <tbody>
{{#mods}}
    <tr>
        <td></td>      
        <td>{{name}}</td>  
        <td><a href="{{url}}" target="_blank">{{# str }} press, block_import_remote_course {{/ str }}</a></td>
        <td>{{time_added}}</td> 
    </tr>
{{/mods}}
</tbody>
</table>
</div>
<div id="acctionsbuttons">
	<hr>
	<button id="resetcount" type="button" class="btn">{{# str }} resetact, block_import_remote_course {{/ str }}</button>
	<button id="close" type="button" class="btn">{{# str }} cancel, block_import_remote_course {{/ str }}</button>  
</div>
</section>