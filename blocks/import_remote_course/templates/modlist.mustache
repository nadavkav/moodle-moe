<section id="modlist">
<form action="/blocks/import_remote_course/import_remote_course.php" method="POST">
    <input type="hidden" name="destcourse" value="{{destcourse}}">
    <table class="table table-striped">
    <thead>
        <tr>
            <th>{{# str }} select, block_import_remote_course {{/ str }}</th>
            <th>{{# str }} type, block_import_remote_course {{/ str }}</th>
            <th>{{# str }} name, block_import_remote_course {{/ str }}</th>
            <th>{{# str }} privewurl, block_import_remote_course {{/ str }}</th>
            <th>{{# str }} timecreate, block_import_remote_course {{/ str }}</th>
        </tr>
    </thead>
    <tbody>
{{#mods}}
    <tr>
        <td><input type="checkbox" name="mod[]" value="{{id}}"></td>
        <td>{{type}}</td>      
        <td>{{name}}</td>  
        <td>{{url}}</td>
        <td>{{time_added}}</td> 
    </tr>
{{/mods}}
</tbody>
</table>
  <input type="submit" value="{{# str }} import, block_import_remote_course {{/ str }}">
</form>
</section>