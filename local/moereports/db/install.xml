<?xml version="1.0" encoding="UTF-8"?>
<XMLDB PATH="mod/questionnaire/db" VERSION="20131005" COMMENT="XMLDB file for Moodle mod/questionnaire"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd">
  <TABLES>
    <TABLE NAME="moereports_reports" COMMENT="Report order by region">
         <FIELDS>
         <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
		  <FIELD NAME="region" TYPE="char" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="Region of school "/>
	      <FIELD NAME="name" TYPE="char" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="school name"/>
	      <FIELD NAME="city" TYPE="char" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="city of school "/>
	    </FIELDS>
	    <KEYS>
              <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
	    </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>