<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/remote_backup_provider/db" VERSION="20160830" COMMENT="XMLDB file for Moodle local/remote_backup_provider"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="remote_backup_provider_subsc" COMMENT="List of subscribers">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="subscriber_name" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="base_url" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="remote_user" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="remote_token" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="remote_backup_provider_fails" COMMENT="List of subscribers">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="url" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="local_params" TYPE="binary" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="options" TYPE="binary" NOTNULL="true" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>