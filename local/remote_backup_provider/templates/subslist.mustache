<section id="subslist">
    <table border="1">
    <thead>
      <tr>
    <th>{{# str }} subscriber_name, local_remote_backup_provider {{/ str }}</th>
    <th>{{# str }} base_url, local_remote_backup_provider {{/ str }}</th>
    <th>{{# str }} remote_user, local_remote_backup_provider {{/ str }}</th>
    <th>{{# str }} remote_token, local_remote_backup_provider {{/ str }}</th>
    <th>{{# str }} remove_remote, local_remote_backup_provider {{/ str }}</th>
        </tr>
    </thead>
    <tbody>
{{#subscribers}}
    <tr>
        <td>{{subscriber_name}}</td>       
        <td>{{base_url}}</td>       
        <td>{{remote_user}}</td> 
        <td>{{remote_token}}</td>
        <td><a href = '{{remove_remote}}'>{{# str }} remove_remote, local_remote_backup_provider {{/ str }}</a></td>                                    
    </tr>
{{/subscribers}}
</tbody>
</table> 
</section>