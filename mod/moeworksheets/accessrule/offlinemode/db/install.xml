<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/moeworksheets/accessrule/offlinemode/db" VERSION="2014110600" COMMENT="XMLDB file for Moodle mod/moeworksheets/accessrule/offlinemode"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="moeworksheetsaccess_offlinmo" COMMENT="Stores the additional setting required by this accessrule">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="moeworksheetsid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="Foreign key to moeworksheets.id."/>
        <FIELD NAME="enabled" TYPE="int" LENGTH="2" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="Boolean, if true, use the fault-tolerant mode."/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="moeworksheetsid" TYPE="foreign-unique" FIELDS="moeworksheetsid" REFTABLE="moeworksheets" REFFIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
