<section id="moeworksheets" class="row-fluid">
    <header class="row-fluid">
        <h2 class="span8">{{quizname}}</h2>
    </header>
    <nav id="navigation" class="row-fluid">
            {{{navigation}}}
            <div class="span5 questionnavbutton">
                <input type="button" title="העמוד הקודם" value="העמוד הקודם" class="mod_moeworksheets-prev-nav-show" />
                <input type="button" title="העמוד הבא" value="העמוד הבא" class="mod_moeworksheets-next-nav-show" />
            </div>
    </nav>
    <section id="questions" class="row-fluid">
        <section id="moeworksheets_question" class="span6">
            <section id="subjectheader" class=span12">
                <h2>{{subject}}</h2>
            </section>
            <section id="questionbox" class="span12"> 
                <form action="{{formacttion}}" method="post" enctype="multipart/form-data" accept-charset="utf-8" id="responseform">
                    <div>
                        {{{slots}}}
                        <input type="hidden" name="attempt" value="{{attempteid}}" />
                        <input type="hidden" name="thispage" value="{{page}}" id="followingpage" />
                        <input type="hidden" name="nextpage" value="{{nextpage}}" />
                        <input type="hidden" name="timeup" value="0" id="timeup" />
                        <input type="hidden" name="sesskey" value="{{sesskey}}" />
                        <input type="hidden" name="scrollpos" value="" id="scrollpos" />
                        <input type="hidden" name="slots" value="{{questionid}}" /> 
                        {{{attemptnavigationbuttons}}}
                        
                    </div>
                </form>
                {{{connectionwarning}}}
            </section>
        </section>
        <section id="addtional_content" class="span6">
            <div class="wraper">
            {{#frame}}
                <iframe src="{{frame}}" id="app"></iframe>
            {{/frame}}
            {{#content}}
                {{#content.css}}
                <style>
                    {{.}}               
                </style>
               {{/content.css}}
               {{#content.html}}
                    {{{.}}}
               {{/content.html}}
               {{#content.javascript}}
                    <script>
                        {{{content.javascript}}}
                    </script>
               {{/content.javascript}}
            {{/content}}
            </div>
        </section>
    </section>
</section>