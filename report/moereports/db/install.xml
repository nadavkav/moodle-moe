<?xml version="1.0" encoding="UTF-8"?>
<XMLDB PATH="report/moereports/db" VERSION="20131005" COMMENT="XMLDB file for Moodle report/moereports"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd">
  <TABLES>
    <TABLE NAME="moereports_reports" COMMENT="Report order by region">
        <FIELDS>
            <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
            <FIELD NAME="symbol" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="symbol of school "/>
		    <FIELD NAME="region" TYPE="char" LENGTH="32" NOTNULL="true" SEQUENCE="false" COMMENT="Region of school "/>
	        <FIELD NAME="name" TYPE="char" LENGTH="32" NOTNULL="true" SEQUENCE="false" COMMENT="school name"/>
	    </FIELDS>
	    <KEYS>
              <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
              <KEY NAME="symboluxi" TYPE="unique" FIELDS="symbol"/>
	    </KEYS>
    </TABLE>
    <TABLE NAME="moereports_reports_classes" COMMENT="Report order by region">
         <FIELDS>
         <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
         <FIELD NAME="symbol" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="symbol of school "/>
		  <FIELD NAME="class" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="class "/>
	      <FIELD NAME="studentsnumber" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="number of students in class"/>
	    </FIELDS>
	    <KEYS>
	   		  <KEY NAME="symbol_fk_fromreports" TYPE="foreign" FIELDS="symbol" REFTABLE="moereports_reports" REFFIELDS="symbol"/>
              <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
	    </KEYS>
    </TABLE>
    <TABLE NAME="moereports_activityregin" COMMENT="per activity regin level data for the reports made by cron">
        <FIELDS>
            <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
            <FIELD NAME="region" TYPE="char" LENGTH="32" NOTNULL="true" SEQUENCE="false" COMMENT="region name"/>            
            <FIELD NAME="course" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" COMMENT="course name"/>
            <FIELD NAME="activityname" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT="activity name"/>
            <FIELD NAME="eighthgradesum" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT="eighth grade sum"/>
            <FIELD NAME="eighthgradetotal" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT="eigh thgrade total"/>
            <FIELD NAME="ninthgradesum" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT="ninth grade sum"/>
            <FIELD NAME="ninthgradetotal" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT="ninth grade total"/>
            <FIELD NAME="tenthgradesum" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT="tenth grade sum"/>
            <FIELD NAME="tenthgradetotal" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT="tenth grade total"/>
        </FIELDS>
        <KEYS>
              <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        </KEYS>
      <INDEXES>
        <INDEX NAME="region" UNIQUE="false" FIELDS="region"/>
      </INDEXES>
    </TABLE>
